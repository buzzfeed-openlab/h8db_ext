{
  "name": "H8DB",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "H8DB Description",
  "homepage_url": "https://todo.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "page_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "H8DB"
  },
  "permissions": [
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}